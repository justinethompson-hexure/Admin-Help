@media print
{
	body
	{
		margin: 8px;
		line-height: normal;
		color: #000000;
		font-family: Calibri;
		font: Calibri;
		font-size: 12pt;
		mc-hyphenate: never;
	}

	Div.GlossaryPageDefinition
	{
		color: #e1e1e1;
		font-size: 9pt;
	}

	Div.GlossaryPageHeading
	{
		border-bottom-color: #bed230;
		border-bottom-style: solid;
		border-bottom-width: 1px;
		color: #bed230;
		font-size: 11pt;
	}

	Div.GlossaryPageTerm
	{
		color: #bed230;
		font-size: 10pt;
		padding-bottom: 10px;
		padding-top: 10px;
	}

	h1
	{
		background-color: transparent;
		border-bottom: none;
		margin-left: 0;
		padding-left: 0px;
		padding-top: 0px;
		font-size: 18pt;
		font-weight: bold;
		color: #000000;
		font-family: Calibri;
		margin-top: 13pt;
		margin-bottom: 3pt;
		line-height: 108%;
		page-break-before: always;
	}

	h1.Appendix
	{
		mc-auto-number-format: 'APPENDIX ';
	}

	h1.UserGuideBHeading
	{
		page-break-before: avoid;
	}

	h2
	{
		font-size: 14pt;
		font-weight: bold;
		color: #000000;
		font-family: Calibri;
		font: Calibri;
		line-height: 108%;
		margin-bottom: 3pt;
		margin-top: 12pt;
		page-break-before: avoid;
	}

	h3
	{
		color: #2e74b5;
		font-weight: bold;
		font-size: 13pt;
		font: Calibri;
		font-family: Calibri;
		margin-top: 14pt;
		margin-bottom: 3pt;
		line-height: 108%;
		mc-heading-level: 3;
	}

	h4
	{
		color: #7f7f7f;
		font: Calibri;
		font-family: Calibri;
		font-size: 13pt;
		font-weight: bold;
		line-height: 108%;
		margin-top: 12pt;
		margin-bottom: 3pt;
		mc-heading-level: 4;
	}

	img.FiftyPercentThumbnail
	{
		max-width: 50%;
	}

	img.OneHundredPercent
	{
		max-width: 100%;
